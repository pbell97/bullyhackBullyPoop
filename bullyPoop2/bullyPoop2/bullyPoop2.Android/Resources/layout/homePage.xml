<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.ContentFrameLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:minWidth="25px"
	android:minHeight="25px"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:id="@+id/contentFrameLayout1" >
	<LinearLayout
		android:orientation="horizontal"
		android:minWidth="25px"
		android:minHeight="25px"
		android:layout_width="match_parent"
		android:layout_height="200px"
		android:id="@+id/linearLayout1">
		<Button
			android:text="Add Review"
			android:layout_width="300px"
			android:layout_height="200px"
			android:id="@+id/button4" />
		<Button
			android:text="Register Bathroom"
			android:layout_width="115.8dp"
			android:layout_height="match_parent"
			android:id="@+id/buttonAddBathroom" />
	</LinearLayout>
    
	<ListView
        android:translationY="200px"
		android:minWidth="25px"
		android:minHeight="25px"
		android:layout_width="match_parent"
		android:layout_height="1300px"
		android:id="@+id/homePageBathroomsList" />

	<LinearLayout
		android:orientation="horizontal"
        android:translationY="1520px"
		android:minWidth="25px"
		android:minHeight="25px"
		android:layout_width="match_parent"
		android:layout_height="200px"
		android:id="@+id/linearLayout2" >
		<Button
			android:text="Home"
			android:layout_width="wrap_content"
			android:layout_height="150px"
			android:id="@+id/buttonHome" />
		<Button
			android:text="Map"
            android:translationX="200px"
			android:layout_width="wrap_content"
			android:layout_height="150px"
			android:id="@+id/buttonMap" />
		<Button
			android:text="Account"
            android:translationX="380px"
			android:layout_width="wrap_content"
			android:layout_height="150px"
			android:id="@+id/buttonAccount" />


    </LinearLayout>
	
	
</android.support.v7.widget.ContentFrameLayout>
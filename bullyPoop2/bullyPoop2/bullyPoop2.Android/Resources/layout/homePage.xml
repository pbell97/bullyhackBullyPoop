<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.ContentFrameLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:minWidth="25px"
	android:minHeight="25px"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:id="@+id/contentFrameLayout1" >
	<LinearLayout
		android:orientation="horizontal"
		android:background = "#800000"
		android:minWidth="25px"
		android:minHeight="25px"
		android:layout_width="match_parent"
		android:layout_height="200px"
		android:id="@+id/linearLayout1">
		<Button
			android:background = "#800000"
			android:text="Add Review"
			android:textColor="#FFFFFF"
			android:textSize="8pt"
			android:layout_width="400px"
			android:layout_height="200px"
			android:id="@+id/buttonAddReviewHomePage" />
        <Button
            android:background = "#800000"
            android:text="Register Bathroom"
			android:textColor="#FFFFFF"
			android:textSize="8pt"
            android:layout_width="720px"
            android:layout_height="match_parent"
            android:id="@+id/buttonAddBathroom"
            android:layout_marginLeft="0.0dp"
            android:layout_marginRight="151.6dp" />
	</LinearLayout>
    
	<ListView
        android:translationY="200px"
		android:minWidth="25px"
		android:minHeight="25px"
		android:layout_width="match_parent"
		android:layout_height="1300px"
		android:id="@+id/homePageBathroomsList" />

	<LinearLayout
		android:background = "#800000"
		android:orientation="horizontal"
        android:translationY="1450px"
		android:minWidth="125px"
		android:minHeight="25px"
		android:layout_width="match_parent"
		android:layout_height="1200px"
		android:id="@+id/linearLayout2" >
		<Button
			android:text=""
			android:background="@drawable/ic_home_active"
			android:layout_width="wrap_content"
			android:layout_height="230px"
			android:id="@+id/buttonHome" />
		<Button
			android:text=""
			android:background="@drawable/ic_map"
            android:translationX="200px"
			android:layout_width="wrap_content"
			android:layout_height="250px"
			android:id="@+id/buttonMap" />
		<Button
			android:text=""
			android:background="@drawable/ic_account"
            android:translationX="380px"
			android:layout_width="wrap_content"
			android:layout_height="250px"
			android:id="@+id/buttonAccount" />
    </LinearLayout>
	
	
</android.support.v7.widget.ContentFrameLayout>